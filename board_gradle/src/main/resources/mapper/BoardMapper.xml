	<select id="getList" resultType="board.vo.BoardVO" >
		SELECT
			  BOARD_ID AS BOARDID
			, TITLE
			, TEXT
			, BOARD_DATE AS BOARDDATE
			, HIT
			, MEMBER_ID AS MEMBERID
		FROM BOARD  
		ORDER BY BOARD_DATE DESC
	</select>
